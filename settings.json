{
    "files.autoSave": "afterDelay",
    "terminal.integrated.shell.windows": "nu.exe",
    "terminal.integrated.automationShell.windows": "cmd.exe",
    "editor.fontFamily": "Atkinson Hyperlegiblee, Merriweather, Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.fontWeight": "300",
    "files.exclude": {
        "node_modules/*/*": true
    },
    "telemetry.enableTelemetry": false,
    "telemetry.enableCrashReporter": false,
    "rust-analyzer.cargo-watch.command": "clippy",
    "workbench.settings.openDefaultKeybindings": true,
    "python.jediEnabled": false,
    "rust-analyzer.rainbowHighlightingOn": true,
    "rust-analyzer.procMacro.enabled": true,
    "rust-analyzer.procMacro.enable": true,
    "rust-analyzer.cargo.loadOutDirsFromCheck": true,
    "todo-tree.tree.showScanModeButton": false,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "trailing",
    "[jsonc]": {},
    "terminal.integrated.fontFamily": "Consolas",
    "rust-analyzer.checkOnSave.command": "clippy",
    "todo-tree.regex.regex": "(($TAGS)|^\\s*- \\[ \\])",
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "todo!"
    ],
    "git.autofetch": true,
    "C_Cpp.updateChannel": "Insiders",
    "editor.suggest.showStatusBar": true,
    "terminal.integrated.rendererType": "experimentalWebgl",
    "editor.minimap.enabled": true,
    "editor.fontSize": 12,
    "workbench.editor.wrapTabs": true,
    "cSpell.userWords": [
        "Asteracea",
        "Deanonymization",
        "Debugless",
        "Dodrio",
        "Dronaroid",
        "Hasher",
        "Paren",
        "Reimplemented",
        "Reorderable",
        "SHOUTY",
        "SNEK",
        "Unerase",
        "Uninit",
        "VDOM",
        "Wasm",
        "ZWNJ",
        "asteracea",
        "asteracea's",
        "autosafe",
        "backtrace",
        "bindgen",
        "bumpalo",
        "clippy",
        "conv",
        "crev",
        "deanonymize",
        "deanonymizes",
        "deprioritized",
        "doctest",
        "featureful",
        "hashbrown",
        "htmlescape",
        "impls",
        "initialisation",
        "interner",
        "markdownlint",
        "miri",
        "monomorphization",
        "newtype",
        "overscroll",
        "recurses",
        "repr",
        "rustfmt",
        "struct",
        "subslice",
        "subtrait",
        "synchronises",
        "taml",
        "toolchain",
        "trunc",
        "underflowed"
    ],
    "errorLens.enabledDiagnosticLevels": [
        "error",
        "warning",
        "info"
    ],
}